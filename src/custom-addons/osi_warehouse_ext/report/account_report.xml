<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Account report search -->
    <record id="view_account_invoice_report_search_rename" model="ir.ui.view">
        <field name="name">account.invoice.report.search.rename</field>
        <field name="model">account.invoice.report</field>
        <field name="inherit_id" ref="account.view_account_invoice_report_search"/>
        <field name="arch" type="xml">
            <xpath expr="//group/filter[@name='account_analytic_id']" position="attributes">
                <attribute name="string">Warehouse Account</attribute>
            </xpath>
        </field>
    </record>

</odoo>