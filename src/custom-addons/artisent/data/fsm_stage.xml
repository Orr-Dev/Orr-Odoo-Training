<odoo noupdate="1">

    <record id="fsm_stage_assigned" model="fsm.stage">
        <field name="name">Assigned</field>
        <field name="sequence">21</field>
        <field name="stage_type">order</field>
        <field name="is_closed">True</field>
        <field name="is_default">True</field>
        <field name="team_ids" eval="[(6,0,[ref('fieldservice.fsm_team_default')])]"/>
        <field name="custom_color">#7F8C8D</field>
    </record>

    <record id="fsm_stage_in_progress" model="fsm.stage">
        <field name="name">In Progress</field>
        <field name="sequence">31</field>
        <field name="stage_type">order</field>
        <field name="is_closed">True</field>
        <field name="is_default">True</field>
        <field name="team_ids" eval="[(6,0,[ref('fieldservice.fsm_team_default')])]"/>
        <field name="custom_color">#7F8C8D</field>
    </record>

    <record id="fsm_stage_review" model="fsm.stage">
        <field name="name">Review</field>
        <field name="sequence">41</field>
        <field name="stage_type">order</field>
        <field name="is_closed">True</field>
        <field name="is_default">True</field>
        <field name="team_ids" eval="[(6,0,[ref('fieldservice.fsm_team_default')])]"/>
        <field name="custom_color">#7F8C8D</field>
    </record>

</odoo>
